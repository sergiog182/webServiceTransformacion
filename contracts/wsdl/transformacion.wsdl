<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<wsdl:definitions xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" 
                  xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" 
                  xmlns:xsd="http://www.w3.org/2001/XMLSchema"
                  xmlns:tns="http://xml.transformacion.com/transformacion" 
                  targetNamespace="http://xml.transformacion.com/transformacion" 
                  xmlns:transformacionIn ="http://xml.transformacion.com/transformacionRequest"
                  xmlns:transformacionOut ="http://xml.transformacion.com/transformacionResponse"
>
    <wsdl:types>
        
        <xsd:schema name="http://xml.transformacion.com/transformacion">
            <xsd:import namespace="http://xml.transformacion.com/transformacionRequest" schemaLocation="http://localhost/webServiceTransformacion/contracts/xsd/transformacionRequest.xsd" />
            <xsd:import namespace="http://xml.transformacion.com/transformacionResponse" schemaLocation="http://localhost/webServiceTransformacion/contracts/xsd/transformacionResponse.xsd" />
        </xsd:schema>
        
    </wsdl:types>
        
    
    <wsdl:message name="transformacionRequest">
        <wsdl:part name="in" element="transformacionIn:transformacionRequest" />
    </wsdl:message>
    <wsdl:message name="transformacionResponse">
        <wsdl:part name="out" element="transformacionOut:transformacionResponse" />
    </wsdl:message>
    
    <wsdl:portType name="transformacion">
        <wsdl:operation name="transformacion">
            <wsdl:input message="tns:transformacionRequest" />
            <wsdl:output message="tns:transformacionResponse" />
        </wsdl:operation>
    </wsdl:portType>    
    
    <wsdl:binding name="transformacionSoap" type="tns:transformacion">
        <soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http" />
        
        <wsdl:operation name="transformacion">
            <soap:operation soapAction="http://xml.transformacion.com/transformacion"/>
            <wsdl:input>
                <soap:body use="literal"/>
            </wsdl:input>
            <wsdl:output>
                <soap:body use="literal"/>
            </wsdl:output>
        </wsdl:operation>
    
    </wsdl:binding>
    
    <wsdl:service name="transformacion">
        <wsdl:port binding="tns:transformacionSoap" name="transformacionSoap">
            <soap:address location="http://localhost/webServiceTransformacion/server.php" />
        </wsdl:port>
    </wsdl:service>
</wsdl:definitions>

